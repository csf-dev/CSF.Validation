{
    "metadata": [
      {
        "src": [
          {
            "src": "../",
            "files": [
              "CSF.Validation.Abstractions/CSF.Validation.Abstractions.csproj",
              "CSF.Validation/CSF.Validation.csproj",
              "CSF.Validation.StandardRules/CSF.Validation.StandardRules.csproj"
            ],
            "exclude": [
              "**/bin/**",
              "**/obj/**"
            ]
          }
        ],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "api/**.yml",
            "api/index.md"
          ]
        },
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md"
          ],
          "exclude": ["**/README.md"]
        }
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "_site",
      "globalMetadata": {
        "_gitContribute": {
          "branch": "master"
        }
      },
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default"
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
  }